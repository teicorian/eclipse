		"@storybook/react-webpack5": "^7.6.5",
		"cross-env": "^7.0.3",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.19.0",
		"react-scripts": "^5.0.1",
		"storybook": "^7.6.5"
