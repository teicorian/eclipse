		"teicor",
		"eclipse",
		"eclipse ui",
		"components",
		"react components",
		"react ui"
	],
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		],
		"plugins": []
	},
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.23.6",
		"@babel/preset-env": "^7.23.6",
