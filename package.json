{
	"name": "@teicor/eclipse",
	"version": "1.7.23",
	"license": "MIT",
	"description": "Modern and minimalist React UI library.",
	"author": "TJ Bredemeyer <tj@teicor.com>",
	"main": "components/index.js",
	"files": [
		"components",
		"README.md",
		"LICENSE.md"
	],
	"scripts": {
		"nightly": "git add -A && yarn version --patch && git push origin nightly",
		"upload": "yarn patch && git add -A && git diff-index --quiet HEAD || git commit -m '.' && git push && yarn build-package && npm publish ",
		"patch": "yarn version --patch",
		"build-package": "npm run clean && npm run babel",
		"babel": "babel src/components --out-dir components --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
		"clean": "rimraf components",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"bugs": {
		"url": "https://github.com/teicorian/eclipse/issues/new/choose"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/teicorian/eclipse.git"
	},
	"homepage": "https://eclipse-ui.com",
	"keywords": [
		"teicor",
		"eclipse",
		"eclipse ui",
		"components",
		"react components",
		"react ui"
	],
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		],
		"plugins": []
	},
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.23.6",
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@babel/preset-env": "^7.23.6",
		"@babel/preset-react": "^7.23.3",
		"cross-env": "^7.0.3",
		"node-fetch": "2",
		"npm-run-path": "^5.2.0",
		"prismjs": "^1.29.0",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-live": "^4.1.5",
		"react-router-dom": "^6.19.0",
		"react-scripts": "^5.0.1",
		"react-simple-code-editor": "^0.13.1"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"resolutions": {
		"strip-ansi": "6.0.0"
	}
}
