{
	"name": "@teicor/eclipse",
	"version": "1.0.11",
	"license": "MIT",
	"description": "Modern and minimalist React UI library.",
	"author": "TJ Bredemeyer <tj@teicor.com>",
	"main": "dist/index.js",
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"dependencies": {
		"babel-preset-react-app": "^10.0.1",
		"cra-template": "1.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.19.0",
		"storybook": "^7.6.5"
	},
	"scripts": {
		"start": "yarn storybook",
		"deploy": "yarn publish && git push origin nightly",
		"publish": "npm run clean && cross-env NODE_ENV=production babel src --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,stories.js,__snapshots__",
		"clean": "rimraf dist",
		"storybook-build": "storybook build",
		"storybook-serve": "storybook && npx serve storybook-static",
		"storybook": "storybook dev -p 6006"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest",
			"plugin:storybook/recommended"
		]
	},
	"bugs": {
		"url": "https://github.com/teicorian/eclipse/issues/new/choose"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/teicorian/eclipse"
	},
	"homepage": "https://eclipse-ui.com",
	"keywords": [
		"teicor",
		"eclipse",
		"eclipse ui",
		"components",
		"react components",
		"react ui"
	],
	"babel": {
		"presets": [
			"@babel/preset-env",
			"@babel/preset-react"
		],
		"plugins": []
	},
	"devDependencies": {
		"@babel/cli": "^7.23.4",
		"@babel/core": "^7.23.6",
		"@babel/preset-env": "^7.23.6",
		"@babel/preset-react": "^7.23.3",
		"@storybook/addon-docs": "^7.6.5",
		"@storybook/addon-essentials": "^7.6.5",
		"@storybook/addon-links": "^7.6.5",
		"@storybook/react-webpack5": "^7.6.5",
		"react-scripts": "^5.0.1",
		"storybook": "^7.6.5"
	}
}
